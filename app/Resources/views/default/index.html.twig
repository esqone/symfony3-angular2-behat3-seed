    <script src="{{ asset('lib/es6-promise.js') }}"></script>
    <script src="{{ asset('lib/es6-shim.js') }}"></script>
    <script src="{{ asset('lib/custom-require.js') }}"></script>
    <script src="{{ asset('lib/system.src.js') }}"></script>
    <script src="{{ asset('app-config.js') }}"></script>
    <script src="{{ asset('lib/Reflect.js') }}"></script>
    <script src="{{ asset('lib/rxjs/Rx.js') }}"></script>
    <script src="{{ asset('lib/angular2/bundles/angular2.dev.js') }}"></script>
    <script src="{{ asset('lib/angular2/bundles/http.dev.js') }}"></script>
    <script src="{{ asset('lib/angular2/bundles/angular2-polyfills.js') }}"></script>
